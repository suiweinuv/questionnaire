<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.QuestionnaireMapper">
	
	 <insert id="insertQuestionnaire" >
           insert into questionnaire(id,company_name , company_people_num,company_add,company_tel,contact_name,contact_phone,inviter,remarks,ip,create_time,status) 
               values(#{id}, #{company_name},#{company_people_num},#{company_add},#{company_tel},#{contact_name},#{contact_phone},#{inviter},#{remarks},#{ip},#{create_time},#{status})
   </insert>
	
</mapper>
